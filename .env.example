# Groq API Key - Get from https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Jenkins URL for automated daily analysis
JENKINS_URL=https://your-jenkins-server.com

# Default model for automated analysis
DEFAULT_MODEL=llama-3.3-70b-versatile

# Flask Environment
FLASK_ENV=development

# Flask Secret Key (change in production!)
FLASK_SECRET_KEY=your-secret-key-change-in-production

# Admin Password Hash (SHA256)
# Default password is "admin123" - CHANGE THIS IN PRODUCTION!
# To generate: python3 -c "import hashlib; print(hashlib.sha256('your_password'.encode()).hexdigest())"
ADMIN_PASSWORD_HASH=240be518fabd2724ddb6f04eeb1da5967448d7e831c08c8fa822809f74c720a9

# Optional: Flask Debug Mode
FLASK_DEBUG=True

# Optional: Flask Port (default: 5000)
FLASK_PORT=5000

# Optional: Database Path (default: jenkins_analysis.db)
DB_PATH=jenkins_analysis.db
